<script>
  import wepy from 'wepy';
  import tip from '@/utils/tip';
  import api from '@/api/api';

  import {
    SYSTEM_INFO,
    CART,
    PAY_ARRAY
  } from '@/utils/constant';

  export default class OrderDetail extends wepy.page {
    config = {
      navigationBarTitleText: '订单详情',
    };

    data = {
      isExistAddress: false,
      addressId: '',
      address: {},
      products: {},
      stores:{},
      storeData:{},
      totalPrice: 0,//商品总价格
      affixation:0,//运费
      promotion:"",
      showInvoice: false,
      invoiceArray: [],
      invoiceIndex: 0,
      shipArray: [{code:1, name:'配送'}, {code:0, name:'自提'}],
      shipIndex: 0,
      payArray:[],
      payIndex: 0,
      winHeight: 0,
      hidden: true,
      payPassword: '',
      errMsg: '',
      orderList: [],
      realPrice: 0,//支付金额（商品总价格+运费）
    };

    clearCart() {
      let cart = wepy.getStorageSync(CART) || {};
      for (let key in this.products) {
        delete cart[key];
      }
      wepy.setStorageSync(CART, cart);
    }

    selectAddress(address) {
      this.address = address;
      this.addressId = address.id;
    }

    async getAddress(){
      let pids = Object.keys(this.products);
      let storeids = Object.keys(this.stores);
      let jsonAddress = await api.defaultAddress(pids,storeids);
      if (jsonAddress.data.stateCode === 0) {
        this.address = jsonAddress.data.data.address;
        this.addressId = this.address.id;
        this.isExistAddress = true;
        this.affixation=jsonAddress.data.affixation;
        // this.promotion=jsonAddress.data.affixation.promotion;
        this.realPrice = this.totalPrice + this.affixation;
        this.$apply();
      } else {
        tip.error(jsonAddress.data.message);
      }
    }
    async getProductInfo() {
      const pids = Object.keys(this.products);
      // const json = await api.cartInfoList(pids,json.data.data.id);
      const json = await api.cartInfoList(pids);
      if (json.data.stateCode === 0 && json.data.stores.length > 0) {
        for(let i=0; i<json.data.stores.length; i++){
          let store = json.data.stores[i];
          this.storeData[store.storeId] = store;
          // this.getInvoiceInfo(store.storeId);
          store.products.forEach((e, i) => {
            this.totalPrice += e.price * this.products[e.id];
          });
        }

        this.$apply();
      } else {
        tip.error(json.data.message);
      }
    }

    // async getInvoiceInfo(storeId) {
    //   const json = await api.invoiceList(storeId);
    //   if (json && json.data && json.data.stateCode === 0) {
    //     this.invoiceArray = ['普通发票'];
    //     this.invoiceIndex = 0;
    //     if (json.data.invoiceType === '1') {
    //       this.invoiceArray.push('增值税发票');
    //     }
    //     this.$apply();
    //   } else {
    //     tip.error(json.data.message);
    //   }
    // }

    components = {};

    onLoad(option) {
      if (option.products) {
        let products = JSON.parse(option.products);
        let stores= JSON.parse(option.stores);
        if (typeof products === 'object' && Object.keys(products).length > 0) {
          this.products = products;
        } else {
          // wepy.navigateBack({
          //   delta: 1,
          // });
          // return;
        }
        if (typeof stores === 'object' && Object.keys(stores).length > 0) {
          this.stores = stores;
        } else {
          // wepy.navigateBack({
          //   delta: 1,
          // });
          // return;
        }

        this.flag = option.flag;
        this.orderNum = option.orderNum;
      }
      const windowHeight = wx.getSystemInfoSync().windowHeight;
      this.winHeight = windowHeight;

      this.getProductInfo();

      this.payArray = PAY_ARRAY;
    }

    onShow() {
      this.getAddress();
    }

    computed = {};

    methods = {
      imgerror(sIndex, pIndex){
        this.storeData[sIndex].products[pIndex].imgUrl = "/images/default.png";
      },
      setAddress() {
        wepy.navigateTo({
          url: "/pages/address?type=order"
        });
      },
      // switchInvoiceChange(e) {
      //   this.showInvoice = e.detail.value;
      // },
      // invoiceTypeSelect(e) {
      //   this.invoiceIndex = parseInt(e.detail.value);
      // },
      shipTypeSelect(e) {
        this.shipIndex = e.detail.value;
      },
      payTypeSelect(e) {
        this.payIndex = parseInt(e.detail.value);
      },
      async saveOrder(e) {
        if (Object.keys(this.products).length === 0) {
          tip.alert('商品信息有误');
          return false;
        }

        if (this.addressId.length === 0) {
          tip.alert('请选择收货地址');
          return false;
        }

        // let invoiceCheck = true;
        // for (let key in e.detail.value) {
        //   if (key !== 'note' && e.detail.value[key].length === 0) {
        //     tip.alert('请填写发票信息');
        //     invoiceCheck = false;
        //     break;
        //   }
        // }

        // if (!invoiceCheck) {
        //   return false;
        // }

        let productList = [];
        for (let pid in this.products) {
          productList.push({
            id: pid,
            qty: this.products[pid],
          });
        }

        let invoiceData = JSON.parse(JSON.stringify(e.detail.value));
        if (!this.showInvoice) {
          invoiceData.type = 0;
        } else {
          if (this.invoiceIndex === 0) {
            invoiceData.type = 1;
          }
          if (this.invoiceIndex === 1) {
            invoiceData.type = 2;
          }
        }

        const json = await
        api.saveOrder(this.address.id, this.shipArray[this.shipIndex].code, this.payArray[this.payIndex].code, invoiceData, e.detail.value.note, productList);
        if (json && json.data) {
          if (json.data.status === '0') {
            let orderList = [];
            let totalCost = 0;
            json.data.subOrders.forEach(e => {
              orderList.push({
              id: e.orderId,
            });
            totalCost += e.productCost;
          });
            this.realPrice = totalCost;
            this.orderList = orderList;
            if (this.payIndex === 0) {
              this.hidden = false;
            } else if (this.payIndex === 1) {
              this.hidden = true;
              //微信支付
              const json = await api.unifiedOrder(this.orderList);
              if (json && json.statusCode === 200) {
                console.log('微信支付接口之前先生成签名成功');
                console.log('签名：' + json.data.data.paySign);
                console.log('随机串：' + json.data.data.nonceStr);
                console.log('时间戳：' + json.data.data.timeStamp);

                let that = this;
                wx.requestPayment({
                  'appId': '',
                  'timeStamp': json.data.data.timeStamp,
                  'nonceStr': json.data.data.nonceStr,
                  'package': json.data.data.package,
                  'signType': 'MD5',
                  'paySign': json.data.data.paySign,
                  'success': function (res) {
                    if (res.errMsg == "requestPayment:ok") {

                      //更新订单支付状态
                      api.updatePayStatus(that.orderList);

                    }
                  },
                  'fail': function (error) {

                  },
                  'complete': function () {
                    wepy.redirectTo({
                      url: '/pages/order',
                    });
                  }

                });
              } else {
                console.log('微信支付接口之前先生成签名失败')
              }
            }
            this.clearCart();
            this.$apply();
          }else {
              const msg = json.data.message ? json.data.message : '下单失败';
              tip.error(msg);
            }
          } else {
            tip.error('下单失败');
          }
        },
      async payMoney() {
        if (!this.payPassword || this.payPassword.length < 6) {
          this.errMsg = '请输入6位支付密码';
          return false;
        }
        this.errMsg = '';
        const json = await api.bestPayOrder(this.payPassword, this.orderList);
        if(json && json.statusCode === 200) {
          const result = json.data;
          if (result.run_pay === 'true') {
            let success = true;
            let message = '';
            for (let i = 0; i < result.order_result.length; i++) {
              if (result.order_result[i].stateCode < 0) {
                success = false;
                message = result.order_result[i].message;
                break;
              }
            }
            if (success) {
              wepy.redirectTo({
                url: "/pages/pay_success?orderId=" + '' + "&totalFee=" + result.total
              });
            } else {
              wepy.redirectTo({
                url: "/pages/pay_success?orderId=" + this.orderId + "&errMsg=" + message
              });
            }
          } else {
            this.errMsg = result.error_message;
            this.$apply();
          }

        } else {
          tip.error(json ? json.errMsg : '')
        }
      },
      closePayPasswordModal() {
        tip.confirm('您可以在我的订单列表中继续支付', {}, '确认取消支付').then(() => {
          wepy.redirectTo({
            url: '/pages/order',
          });
        });
      },
      passwordInput(e) {
        this.payPassword = e.detail.value;
      },
    };

    events = {}

  }

</script>
